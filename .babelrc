{
  "presets": ["env", "react", "flow", "stage-2"],
  "plugins" : [
    "transform-flow-comments",
    ["module-resolver", {
      "root": ["."],
        "alias": {
          "~": "./"
        }
      }
    ],
    [
      "import", {
        "libraryName": "antd",
        "style": false
      }
    ]
  ]
}
